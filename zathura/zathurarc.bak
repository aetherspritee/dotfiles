### zathura-pywal ###
# Allow recolor
set recolor 'true'

# Don't allow original hue when recoloring
set recolor-keephue 'false'

# Don't keep original image colors while recoloring
set recolor-reverse-video 'false'

# Command line completion entries
set completion-fg '#c1c1c0'
set completion-bg '#0a0705'

# Command line completion group elements
set completion-group-fg '#EAAE3E'
set completion-group-bg '#0a0705'

# Current command line completion element
set completion-highlight-fg '#0a0705'
set completion-highlight-bg '#c1c1c0'

# Default foreground/background color
set default-bg rgba(10,7,5,1)

# Input bar
set inputbar-fg '#c1c1c0'
set inputbar-bg '#0a0705'

# Notification
set notification-fg '#c1c1c0'
set notification-bg '#0a0705'

# Error notification
set notification-error-fg '#c1c1c0'
set notification-error-bg '#957354'

# Warning notification
set notification-warning-fg '#c1c1c0'
set notification-warning-bg '#957354'

# Tab - TODO
# set tabbar-fg
# set tabbar-bg

# Focused tab - TODO
# set tabbar-focus-fg
# set tabbar-focus-bg

# Status bar
set statusbar-fg '#c1c1c0'
set statusbar-bg '#0a0705'

# Highlighting parts of the document (e.g. show search results)
set highlight-color '#EAAE3E'
set highlight-active-color '#EAAE3E'

# Represent light/dark colors in recoloring mode
set recolor-lightcolor rgba(0,0,0,0)
set recolor-darkcolor '#c1c1c0'

# 'Loading...' text
set render-loading-fg '#c1c1c0'
set render-loading-bg '#0a0705'

# Index mode
set index-fg '#c1c1c0'
set index-bg '#0a0705'

# Selected element in index mode
set index-active-fg '#0a0705'
set index-active-bg '#c1c1c0'